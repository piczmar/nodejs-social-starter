{
  "name": "nodejs-social-starter",
  "version": "0.0.1",
  "description": "",
  "main": "server.js",
  "scripts": {
    "build": "babel src -d build --copy-files",
    "start": "npm run build && cross-env NODE_ENV=development node build/server.js | node_modules/bunyan/bin/bunyan",
    "start:watch": "cross-env NODE_ENV=development WATCH=true babel-watch -w src | node_modules/bunyan/bin/bunyan",
    "lint": "eslint --cache --format=node_modules/eslint-formatter-pretty .",
    "lint-fix": "npm run lint -- --fix"
  },
  "author": "Marcin Piczkowski",
  "license": "ISC",
  "dependencies": {
    "bunyan": "^1.8.12",
    "express": "~4.9.x",
    "mongodb": "^3.0.4",
    "restify": "^2.8.4"
  },
  "engines": {
    "node": "0.10.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/piczmar/nodejs-social-starter"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-preset-env": "^1.6.1",
    "babel-watch": "^2.0.7",
    "cli-table": "^0.3.1",
    "cross-env": "^5.1.4",
    "eslint": "^4.19.1",
    "eslint-formatter-pretty": "^1.3.0",
    "eslint-plugin-compat": "^2.2.0",
    "eslint-plugin-promise": "^3.7.0"
  }
}
